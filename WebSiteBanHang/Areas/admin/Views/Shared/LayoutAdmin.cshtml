<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>HubMobile Admin</title>

    <!-- Bootstrap Core CSS -->
    <link href="~/Areas/admin/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- MetisMenu CSS -->
    <link href="~/Areas/admin/assets/css/metisMenu.min.css" rel="stylesheet" />
    <script src="~/Areas/admin/assets/js/jquery.min.js"></script>
    <!-- Timeline CSS -->
    <link href="~/Areas/admin/assets/css/timeline.css" rel="stylesheet" />

    <!-- Custom CSS -->
    <link href="~/Areas/admin/assets/css/startmin.css" rel="stylesheet" />

    <!-- Morris Charts CSS -->
    <link href="~/Areas/admin/assets/css/morris.css" rel="stylesheet" />

    <!-- Custom Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins">

    <link href="~/Areas/admin/assets/css/font-awesome.min.css" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
            <ul class="nav navbar-nav navbar-left navbar-top-links">
                <li><a href="@Url.Action("Index", "ThongKe")"><i class="fa fa-home fa-fw"></i> Trang chủ </a></li>
            </ul>


            <ul class="nav navbar-right navbar-top-links">
                <li class="dropdown navbar-inverse">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-bell fa-fw"></i> <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu dropdown-alerts">
                        <li>
                            <a href="#">
                                <div>
                                    <i class="fa fa-comment fa-fw"></i> New Comment
                                    <span class="pull-right text-muted small">4 minutes ago</span>
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <div>
                                    <i class="fa fa-twitter fa-fw"></i> 3 New Followers
                                    <span class="pull-right text-muted small">12 minutes ago</span>
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <div>
                                    <i class="fa fa-envelope fa-fw"></i> Message Sent
                                    <span class="pull-right text-muted small">4 minutes ago</span>
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <div>
                                    <i class="fa fa-tasks fa-fw"></i> New Task
                                    <span class="pull-right text-muted small">4 minutes ago</span>
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <div>
                                    <i class="fa fa-upload fa-fw"></i> Server Rebooted
                                    <span class="pull-right text-muted small">4 minutes ago</span>
                                </div>
                            </a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a class="text-center" href="#">
                                <strong>See All Alerts</strong>
                                <i class="fa fa-angle-right"></i>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-user fa-fw"></i>@Session["TaiKhoan1"].ToString()<b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu dropdown-user">
                        <li>
                            <a href="#"><i class="fa fa-user fa-fw"></i>Thông tin tài khoản</a>
                        </li>

                        <li class="divider"></li>
                        <li>
                            <a href="@Url.Action("DangXuat", "Authh")"><i class="fa fa-sign-out fa-fw"></i> Đăng xuất</a>
                        </li>
                    </ul>
                </li>
            </ul>
            <!-- /.navbar-top-links -->

            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li class="sidebar-search">
                            <div class="input-group custom-search-form">
                                <input type="text" class="form-control" placeholder="Search...">
                                <span class="input-group-btn">
                                    <button class="btn btn-primary" type="button">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                            <!-- /input-group -->
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "ThongKe")" class="active"><i class="fa fa-dashboard fa-fw"></i>Thống kê</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-bar-chart-o fa-fw"></i> Khách hàng / Thành viên<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="@Url.Action("DanhSachKhachHang", "KhachHang")">Danh sách khách hàng</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("DanhSachThanhVien", "ThanhVien")">Danh sách thành viên</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                      
                        <li>
                            <a href="#"><i class="fa fa-bar-chart-o fa-fw"></i> Sản phẩm<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="@Url.Action("Index", "QuanLySanPham")">Danh sách sản phẩm</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("TaoMoi", "QuanLySanPham")">Thêm mới sản phẩm</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            <a href="@Url.Action("Index","PhanQuyen")"><i class="fa fa-table fa-fw"></i> Phân quyền</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-bar-chart-o fa-fw"></i>Đơn hàng<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="@Url.Action("ChuaThanhToan", "QuanLyDonHang")">Duyệt đơn hàng</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("ChuaGiao", "QuanLyDonHang")">Đơn hàng chưa giao</a>
                                </li>
                               
                                <li>
                                    <a href="@Url.Action("DaGiaoDaThanhToan", "QuanLyDonHang")">Đơn hàng đã giao</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>

                        @*<li>
                            <a href="#"><i class="fa fa-sitemap fa-fw"></i> Nhập hàng / Xuất hàng<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="#">Nhập hàng <span class="fa arrow"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="#">Phiếu nhập</a>
                                        </li>

                                    </ul>
                                    <a href="#">Xuất hàng <span class="fa arrow"></span></a>
                                    <ul class="nav nav-third-level">
                                        <li>
                                            <a href="#">Phiếu xuất</a>
                                        </li>

                                    </ul>
                                    <!-- /.nav-third-level -->
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>*@

                    </ul>
                </div>
            </div>
        </nav>

        <div id="page-wrapper">
            <div class="container-fluid">

                @RenderBody()
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.4.1.js"
            integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
            crossorigin="anonymous"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="~/Areas/admin/assets/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Areas/admin/assets/js/metisMenu.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="~/Areas/admin/assets/js/raphael.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.min.js"></script>
    @*<script src="~/Areas/admin/assets/js/morris.min.js"></script>*@
    @*<script src="~/Areas/admin/assets/js/morris-data.js"></script>*@

    <!-- Custom Theme JavaScript -->
    <script src="~/Areas/admin/assets/js/startmin.js"></script>
